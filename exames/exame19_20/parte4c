0x1018 	 0x1000

movl $vec, %edx					edx=0x1000
movl $2, %ecx					ecx=2
leal (%edx, %ecx, 4), %eax		eax=&(0x1000+ 4*2)=0x1008
movl (%eax, %ecx, 4), %eax		eax=0x1008[8]=0x1008

Resposta: 0x1008
